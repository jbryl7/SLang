
print("start interpreting")

class XD(x: Int, h: String) {
    def foo(arg1: Int): Unit = print("foo method of XD class")
}

class Inna(x: XD) {}

var obiekt: XD = XD(10, "some text")
var innyObiekt: Inna = Inna(obiekt)
print("XD obiekt -> " + obiekt)
print("Inna obiekt -> " + innyObiekt)
var s: String = "\" yo buddy \"" // comment
var x: Int = 7

def fibonacci(n: Int): Int = {
    if (n < 2)
        return 1
    else
        return fibonacci(n-1) + fibonacci(n-2)
}

print("fib: " + fibonacci(x))
x = 10
print(x)