
print("start interpreting")

class XD(x: Int, h: String) {
    def x(arg1: Int): Unit = print("klasa XD metoda x")
}

class Inna(x: XD) {}

var obiekt: XD = XD(10, "jakis napis")
var innyObiekt: Inna = Inna(obiekt)

