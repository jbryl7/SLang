def foo(y: Int): Int = {
    print(y)
    if (x == "x") {
        return foo(1)
    } else
        return "rekurencja zadzialala"
}

def counter(x: Int): Unit = {
    if (x >= 0){
        print(x)
        counter(x-1)
     }
}

counter(10)
